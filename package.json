    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "jquery": "^3.1.1",
    "postscribe": "^2.0.8",
    "react": "^15.4.0",
    "react-dom": "^15.4.0"
  },
  "scripts": {
    "start": "webpack-dev-server  --config ./webpack.dev.config.js --inline --hot --display-error-details  --history-api-fallback  --progress --colors --port 5000 --host 0.0.0.0",
    "build": "webpack --config ./webpack.prod.config.js",
    "test": "jest",
    "eslint": " ./node_modules/.bin/eslint . --fix",
    "precommit": "npm run eslint && npm test",
    "deploy-to-prod": "",
    "deploy-to-test": ""
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy"
    },
    "cacheDirectory": "/tmp/jest/",
    "collectCoverage" : true,
    "collectCoverageFrom": ["src/app/**/*", "!**/node_modules/**", "!**/vendor/**"]
  }
}
